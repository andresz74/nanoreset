  },
  "homepage": "https://github.com/tiaanduplessis/nanoreset",
  "bugs": "https://github.com/tiaanduplessis/nanoreset/issues",
  "author": {
    "name": "Tiaan",
    "email": "tiaanduplessis@hotmail.com",
    "url": "tiaan.beer"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "csscomb nanoreset.css",
